{
  "name": "cmsdata-client",
  "version": "1.1.1",
  "description": "A JavaScript client for CMS Data",
  "repository": {
    "type": "git",
    "url": "https://github.com/consensusnetworks/cmsdata-client"
  },
  "main": "index.js",
  "scripts": {
    "doc": "jsdoc -d docs index.js",
    "test": "tap --no-coverage --reporter=specy -J",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "test:report": "tap --coverage-report=html index.test.js",
    "build:browser": "esbuild index.js --outdir=build --bundle --sourcemap --minify --target=chrome58,firefox57,safari11,edge16"
  },
  "keywords": [
    "cms",
    "cms client",
    "medical data"
  ],
  "author": "Consensus Networks",
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "detect-node": "^2.1.0",
    "esbuild": "^0.12.8",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jsdoc": "^3.6.7",
    "prettier": "^2.2.1",
    "tap": "^15.0.6"
  }
}
