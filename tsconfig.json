{
  "compilerOptions": {
    "outDir": "./build",
	"lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
	"strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
	"noImplicitThis": true,
	"sourceMap": true, 
	"resolveJsonModule": true,
	"noUnusedParameters": true, 
	"noFallthroughCasesInSwitch": true,
	"forceConsistentCasingInFileNames": true,
  },
  "include": ["./src/*"]
}